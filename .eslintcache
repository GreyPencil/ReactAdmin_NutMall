story.goBack()}\r\n                    />\r\n                </LinkButton>\r\n\r\n                <span>Products Details</span>\r\n            </span>\r\n\r\n        )\r\n\r\n\r\n        return (\r\n            <Card title={title} className='product-detail'>\r\n                <List>\r\n                    <Item>\r\n                        <span className=\"left\">Product Name:</span>\r\n                        <span>{name}</span>\r\n                    </Item>\r\n                    <Item>\r\n                        <span className=\"left\">Product Description: </span>\r\n                        <span>{desc}</span>\r\n                    </Item>\r\n                    <Item>\r\n                        <span className=\"left\">Product Price: </span>\r\n                        <span>$ {price}</span>\r\n                    </Item>\r\n                    <Item>\r\n                        <span className=\"left\">Category: </span>\r\n                        <span>{cName1} {cName2 ? ' --> '+cName2 : ''}</span>\r\n                    </Item>\r\n                    <Item>\r\n                        <span className=\"left\">Product Image:</span>\r\n                        <span>\r\n              {\r\n                  imgs.map(img => (\r\n                      <img\r\n                          key={img}\r\n                          src={BASE_IMG_URL + img}\r\n                          className=\"product-img\"\r\n                          // style={{width:150, height: 150, marginRight:10, border: '1px solid #002140'}}\r\n                          alt=\"img\"\r\n                      />\r\n                  ))\r\n              }\r\n            </span>\r\n                    </Item>\r\n                    <Item>\r\n                        <span className=\"left\">Product Details: </span>\r\n                        <span dangerouslySetInnerHTML={{__html: detail}}>\r\n                        </span>\r\n                    </Item>\r\n\r\n                </List>\r\n            </Card>\r\n        )\r\n    }\r\n}","C:\\Code\\Workspace_NutMall\\react-admin_client\\src\\pages\\product\\home.jsx",[],"C:\\Code\\Workspace_NutMall\\react-admin_client\\src\\utils\\constant.js",[],"C:\\Code\\Workspace_NutMall\\react-admin_client\\src\\pages\\product\\add-update.jsx",[],"C:\\Code\\Workspace_NutMall\\react-admin_client\\src\\pages\\product\\rich-text-editor.jsx",[],"C:\\Code\\Workspace_NutMall\\react-admin_client\\src\\pages\\product\\pictures-wall.jsx",[],"C:\\Code\\Workspace_NutMall\\react-admin_client\\src\\pages\\role\\add-form.jsx",[],"C:\\Code\\Workspace_NutMall\\react-admin_client\\src\\pages\\role\\auth-form.jsx",[],"C:\\Code\\Workspace_NutMall\\react-admin_client\\src\\utils\\constants.js",[],"C:\\Code\\Workspace_NutMall\\react-admin_client\\src\\pages\\order\\order.jsx",[],"C:\\Code\\Workspace_NutMall\\react-admin_client\\src\\pages\\not-found\\not-found.jsx",[],"C:\\Code\\Workspace_NutMall\\react-admin_client\\src\\pages\\home\\bar.jsx",[],"C:\\Code\\Workspace_NutMall\\react-admin_client\\src\\pages\\home\\line.jsx",[],"C:\\Code\\Workspace_NutMall\\react-admin_client\\src\\pages\\user\\user-form.jsx",[],{"ruleId":"172","replacedBy":"173"},{"ruleId":"174","replacedBy":"175"},{"ruleId":"176","severity":1,"message":"177","line":142,"column":29,"nodeType":"178","endLine":142,"endColumn":70},{"ruleId":"179","severity":1,"message":"180","line":5,"column":1,"nodeType":"181","endLine":7,"endColumn":2},{"ruleId":"179","severity":1,"message":"180","line":6,"column":1,"nodeType":"181","endLine":20,"endColumn":2},{"ruleId":"182","severity":1,"message":"183","line":3,"column":9,"nodeType":"184","messageId":"185","endLine":3,"endColumn":20},{"ruleId":"182","severity":1,"message":"186","line":1,"column":16,"nodeType":"184","messageId":"185","endLine":1,"endColumn":25},{"ruleId":"182","severity":1,"message":"187","line":1,"column":27,"nodeType":"184","messageId":"185","endLine":1,"endColumn":35},{"ruleId":"182","severity":1,"message":"188","line":1,"column"